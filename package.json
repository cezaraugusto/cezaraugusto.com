    "lint:css": "stylelint --config='config/.stylelintrc.json' 'src/_includes/assets/css/*.css'",
    "build:staging": "cross-env NODE_ENV=staging npm run 11ty",
    "build:production": "cross-env NODE_ENV=production npm run 11ty"
